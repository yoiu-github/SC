{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "lock_periods",
    "max_payments",
    "nft_contract",
    "nft_contract_hash",
    "tier_contract",
    "tier_contract_hash",
    "token_contract",
    "token_contract_hash"
  ],
  "properties": {
    "admin": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "lock_periods": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint64",
        "minimum": 0.0
      }
    },
    "max_payments": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Uint128"
      }
    },
    "nft_contract": {
      "$ref": "#/definitions/HumanAddr"
    },
    "nft_contract_hash": {
      "type": "string"
    },
    "tier_contract": {
      "$ref": "#/definitions/HumanAddr"
    },
    "tier_contract_hash": {
      "type": "string"
    },
    "token_contract": {
      "$ref": "#/definitions/HumanAddr"
    },
    "token_contract_hash": {
      "type": "string"
    },
    "whitelist": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/HumanAddr"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
