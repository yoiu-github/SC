{
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ],
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true
  }
}
